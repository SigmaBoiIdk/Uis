        Tracer = false,
        LookAt = false,
        VisibleCheck = false,
        Key = 'c',
        healthonplr = false,
        AutoSetup = false,
        Ben = false,
        GoTo = false,
        Highlight = false,
        HighlightFill = Color3.fromRGB(255,255,255),
        HighlightOutline = Color3.fromRGB(251,255,255),
